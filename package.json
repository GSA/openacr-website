{
  "name": "openacr-website",
  "version": "0.1.0",
  "scripts": {
    "build": "bundle exec jekyll build",
    "clean": "bundle exec jekyll clean",
    "reset": "npx rimraf .git",
    "start": "bundle exec jekyll serve",
    "start-detached": "bundle exec jekyll serve --detach",
    "htmlproofer": "bundle exec htmlproofer --allow-hash-href _site",
    "pa11y-ci:sitemap": "pa11y-ci --sitemap http://127.0.0.1:4000/sitemap.xml --sitemap-exclude \"/*.pdf\"  --sitemap-exclude \"/admin\""
  },
  "dependencies": {
    "netlify-cms": "^2.10.183",
    "uswds": "~2.13.0"
  },
  "devDependencies": {
    "pa11y-ci": "^3.0.1",
    "rimraf": "^3.0.2"
  }
}
